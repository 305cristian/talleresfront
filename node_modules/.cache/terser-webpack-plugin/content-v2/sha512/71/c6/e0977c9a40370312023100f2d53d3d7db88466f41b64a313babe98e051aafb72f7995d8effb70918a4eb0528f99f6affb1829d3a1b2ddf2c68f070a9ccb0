m'react';\n//import {render}from 'react-dom';\nimport {Table, Button, Container, Modal, ModalHeader, ModalBody, ModalFooter, formGroup, CardTitle, Card, CardBody, CardHeader, CardImg, CardText}from 'reactstrap';\nimport axios from 'axios';\n\nexport default class TallerList extends Component {\n    constructor() {\n        super();\n        this.state = {\n            talleres: []\n        }\n    }\n\n    componentDidMount() {\n        this.getTaller();\n    }\n    getTaller() {\n        axios.get('http://localhost:4000/api/talleres').then(response => {\n            console.log(response.data)\n            this.setState({talleres: response.data})\n\n        })\n\n    }\n\n    render() {\n        return (\n                <Container>\n                    <div className=\"row my-5\">\n                \n                        {\n                            this.state.talleres.map((taller) => (\n                                        <div className=\"col-md-3\" key={taller._id}>\n                                            <CardHeader>\n                                                <CardTitle className=\"font-weight-bold\" tag=\"h5\">{taller.title}</CardTitle>\n                                            </CardHeader>\n                                            <CardBody className=\"text-center\">\n                                                <CardImg top width=\"100%\" src=\"/assets/318x180.svg\" alt=\"Card image cap\" /> \n                                                <CardText>{taller.description}</CardText>\n                                                <Button color=\"secondary\">Ver Taller</Button>\n                                            </CardBody>\n                                        </div>\n                                        ))\n                        }\n                \n                \n                \n                    </div>\n                </Container>\n                )\n    }\n}\n\n\n","import React, {Component} from'react';\nimport {render}from'react-dom';\nimport {Table, Button, Container, Modal, ModalHeader, ModalBody, ModalFooter, formGroup}from 'reactstrap';\nimport Swal from 'sweetalert';\nimport axios from 'axios'\n\nexport default class AdminTalleres extends Component {\n    constructor() {\n        super();\n        this.state = {\n            title: '',\n            description: '',\n            talleres: [],\n            _id: '',\n            textButton: 'REGISTRAR',\n            modalOpen: false\n        };\n        this.addTaller = this.addTaller.bind(this);\n        this.handleChange = this.handleChange.bind(this);\n        this.showModal = this.showModal.bind(this);\n        this.hideModal = this.hideModal.bind(this);\n    }\n\n    componentDidMount() {\n        this.getTalleres();\n    }\n\n    getTalleres() {\n        axios.get('http://localhost:4000/api/talleres').then((response) => {\n            console.log(response.data)\n            this.setState({talleres: response.data})\n        })\n    }\n    addTaller(e) {\n        const datos = {\n            title: this.state.title,\n            description: this.state.description\n        }\n        if (this.state._id) {\n            e.preventDefault();\n            axios.put('http://localhost:4000/api/talleres/' + this.state._id, datos).then((data) => {\n                console.log(data);\n                Swal({\n                    title: 'Tarea Actualizada',\n                    text: 'ok',\n                    icon: 'success',\n                    timer: 2000,\n                    button: false\n                });\n                this.getTalleres();\n                this.setState({title: '', description: '', _id: '', textButton: 'REGISTRAR', modalOpen: false});\n            }).catch(err => console.error(err));\n\n\n        } else {\n            e.preventDefault();\n            axios.post('http://localhost:4000/api/talleres', datos).then((data) => {\n                console.log(data);\n                Swal({\n                    title: 'Tarea Registrada',\n                    text: 'ok',\n                    icon: 'success',\n                    timer: 2000,\n                    button: false\n                });\n                this.getTalleres()\n                this.setState({title: '', description: ''})\n            })\n                    .catch(err => console.error(err));\n//        console.log(this.state);\n\n        }\n    }\n    deleteTaller(id) {\n        Swal({\n            title: 'Esta seguro de eliminar la tarea',\n            text: ' La tarea se eliminara definitivamente',\n            icon: 'warning',\n            buttons: ['Cancelar', 'Sí'],\n            dangerMode: true\n        }).then((value) => {\n            if (value) {\n                axios.delete('http://localhost:4000/api/talleres/'+id) .then((response) => {\n                            console.log(response.data);\n                            Swal({\n                                title: 'Tarea eliminada',\n                                icon: 'success',\n                                timer: 1000,\n                                button: false\n                            });\n                            this.getTalleres();\n                        });\n            }\n        });\n\n\n    }\n\n    editTaller(id) {\n        axios.get('http://localhost:4000/api/talleres/' + id).then((response) => {\n            console.log(response.data);\n            this.setState({\n                title: response.data.title,\n                description: response.data.description,\n                _id: response.data._id,\n                textButton: 'ACTUALIZAR'\n            });\n        })\n\n    }\n    hideModal() {\n        this.setState({title: '', description: '', _id: '', textButton: 'REGISTRAR', modalOpen: false});\n    }\n    showModal() {\n        this.setState({modalOpen: true});\n    }\n\n    handleChange(e) {\n//        console.log(e.target);\n        const{name, value} = e.target;\n        this.setState({\n            [name]: value\n        });\n    }\n\n    render() {\n        return (\n                <div>                   \n                \n                    <Container>\n                        <br/>\n                        <Button color=\"primary\" onClick={this.showModal}>Nueva Tarea</Button>\n                        <br/>\n                        <br/>\n                        <Table>\n                            <thead>\n                                <tr>\n                                    <th>Tarea</th>\n                                    <th>Descripcion</th>\n                                    <th>Acciones</th>\n                                </tr>\n                            </thead>\n                            <tbody>\n                                {\n                                    this.state.talleres.map(data => {\n                                        return(\n                                        <tr key={data._id}>\n                                            <td>{data.title}</td>\n                                            <td>{data.description}</td>\n                                            <td>\n                                                <Button color=\"warning\" onClick={() => {\n                                                                        this.showModal();\n                                                                        this.editTaller(data._id);\n\n                                                                            }}>Edit</Button>{' '}\n                                                <Button color=\"danger\" onClick={ () => {\n                                                                        this.deleteTaller(data._id)\n                                                                            }}>Delete</Button>\n                                            </td>\n                                        </tr>\n                                                )\n           